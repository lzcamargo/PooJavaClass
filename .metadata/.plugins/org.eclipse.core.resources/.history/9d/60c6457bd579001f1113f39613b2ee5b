package desafio;

public class Condutor {
	private String nomeCondutor;
	private long cpfCondutor;
	private long foneCondutor;
	private Veiculo veiculos;
	
	public Condutor(String nomeCondutor, long cpfCondutor, long foneCondutor, Veiculo veiculos) {
		this.nomeCondutor = nomeCondutor;
		this.cpfCondutor = cpfCondutor;
		this.foneCondutor = foneCondutor;
		this.veiculos = veiculos;
	}

	public String getNomeCondutor() {
		return nomeCondutor;
	}

	public long getCpfCondutor() {
		return cpfCondutor;
	}

	public long getFoneCondutor() {
		return foneCondutor;
	}

	public Veiculo getVeiculos() {
		return veiculos;
	}
	
	
	
	
	

}
